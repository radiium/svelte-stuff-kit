{
    "name": "svelte-stuff-kit",
    "description": "Svelte ui components and other stuff",
    "version": "0.0.2",
    "license": "MIT",
    "homepage": "https://github.com/radiium/svelte-stuff-kit",
    "keywords": [
        "svelte",
        "sveltekit",
        "ui",
        "components",
        "components library",
        "phosphore",
        "typescript"
    ],
    "author": {
        "name": "radiium",
        "email": "remip.dev@gmail.com",
        "url": "https://github.com/radiium"
    },
    "bugs": {
        "url": "https://github.com/radiium/svelte-stuff-kit/issues"
    },
    "svelte": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "type": "module",
    "scripts": {
        "dev": "vite dev",
        "build": "vite build && npm run package",
        "preview": "vite preview",
        "package": "svelte-kit sync && svelte-package && publint",
        "prepublishOnly": "npm run package",
        "test": "npm run test:integration && npm run test:unit",
        "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
        "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
        "lint": "prettier --check . && eslint .",
        "format": "prettier --write .",
        "test:integration": "playwright test",
        "test:unit": "vitest"
    },
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "svelte": "./dist/index.js"
        }
    },
    "files": [
        "dist",
        "!dist/**/*.test.*",
        "!dist/**/*.spec.*"
    ],
    "peerDependencies": {
        "svelte": "^4.0.0"
    },
    "devDependencies": {
        "@el3um4s/svelte-get-component-info": "^0.0.16",
        "@playwright/test": "^1.40.1",
        "@sveltejs/adapter-auto": "^2.0.0",
        "@sveltejs/adapter-static": "^2.0.3",
        "@sveltejs/adapter-vercel": "^3.1.0",
        "@sveltejs/kit": "^1.27.7",
        "@sveltejs/package": "^2.0.0",
        "@typescript-eslint/eslint-plugin": "^6.13.2",
        "@typescript-eslint/parser": "^6.13.2",
        "eslint": "^8.55.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-svelte": "^2.30.0",
        "highlight.js": "^11.9.0",
        "prettier": "^3.0.0",
        "prettier-plugin-svelte": "^3.0.0",
        "publint": "^0.1.9",
        "sass": "^1.69.5",
        "svelte": "^4.2.8",
        "svelte-check": "^3.6.0",
        "ts-morph": "^21.0.1",
        "tslib": "^2.4.1",
        "typescript": "^5.3.3",
        "vite": "^4.4.2",
        "vitest": "^0.34.6"
    },
    "dependencies": {
        "@floating-ui/dom": "^1.5.3",
        "phosphor-svelte": "^1.3.0",
        "svelte-stuff-kit": "github:radiium/svelte-stuff-kit"
    }
}
